        <xsl:if test="count(rg:CandidateAndResult[position() = 1]/rg:VotesInElectoralDistrictPart[position() = 1]/rg:VotesInElectoralDistrict) &lt;= 4">
            <xsl:call-template name="PartyHeaderEvenOdd">
