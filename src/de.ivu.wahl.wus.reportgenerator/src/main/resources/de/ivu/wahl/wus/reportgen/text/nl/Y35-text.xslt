<?xml version="1.0" encoding="UTF-8"?>
<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
    <xsl:param name="Y35_010_pretitle" select="'Model Y 35'"/>
    <xsl:param name="Y35_020_title" select="'Verklaring dat u in de lidstaat van uw herkomst niet bent uitgesloten van het recht om gekozen te worden/Declaration of non-exclusion from the right to stand for election in your home Member State'"/>
    <xsl:param name="Y35_030_MetDitFormulier" select="'Met dit formulier verklaart u, als onderdaan van een andere lidstaat van de Europese Unie, dat u in die lidstaat niet bent uitgesloten van het recht om gekozen te worden. By this form, you declare, as subject of another Member State of the European Union, that you are not excluded from the right to stand for election in your home Member State.'"/>
    <xsl:param name="Y35_040_GegevensKandidaat" select="'Gegevens kandidaat /Candidate information'"/>
    <xsl:param name="Y35_050_naam" select="'Naam/name'"/>
    <xsl:param name="Y35_060_voornaamVoorletters" select="'voornaam en overige voorletters/given name and initials'"/>
    <xsl:param name="Y35_070_adres" select="'adres/address'"/>
    <xsl:param name="Y35_080_woonplaats" select="'woonplaats/place of residence'"/>
    <xsl:param name="Y35_090_nationaliteit" select="'nationaliteit/nationality'"/>
    <xsl:param name="Y35_100_geboortedatum" select="'geboortedatum/date of birth'"/>
    <xsl:param name="Y35_110_geboorteplaats" select="'geboorteplaats/place of birth'"/>
    <xsl:param name="Y35_120_laatsteAdres" select="'laatste adres en woonplaats in de lidstaat van herkomst/last address and place of residence in the home member state'"/>
    <xsl:param name="Y35_130_Ondertekening" select="'Ondertekening door de kandidaat/Signature by the candidate'"/>
    <xsl:param name="Y35_140_IkVerklaar" select="'Ik verklaar dat ik niet ben uitgesloten van het recht om gekozen te worden in de lidstaat van mijn herkomst. / I hereby declare that I am not excluded from the right to stand for election in my home Member State.'"/>
    <xsl:param name="Y35_150_Datum" select="'Datum/Date'"/>
    <xsl:param name="Y35_160_Handtekening" select="'Handtekening/Signature'"/>
</xsl:stylesheet>
