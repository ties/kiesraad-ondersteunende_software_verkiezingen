ext>                                                             </fo:inline>                                                                <fo:block/>                                                         </fo:block>                                                     </fo:table-cell>                                                    </fo:table-row>                                             </xsl:variable>                                             <xsl:choose>                                                    <xsl:when test="string($altova:tablerows)">                                                     <xsl:copy-of select="$altova:tablerows"/>                                                   </xsl:when>                                                 <xsl:otherwise>                                                     <fo:table-row>                                                          <fo:table-cell>                                                             <fo:block/>                                                         </fo:table-cell>                                                        </fo:table-row>                                                 </xsl:otherwise>                                                </xsl:choose>                                           </fo:table-body>                                        </fo:table>                                     <fo:block>                                          <fo:leader leader-pattern="space"/>                                     </fo:block>                                 </fo:block>                                 <fo:block>                                      <fo:leader leader-pattern="space"/>                                 </fo:block>                                 <fo:inline-container>                                       <fo:block>                                          <xsl:text>&#x200B;</xsl:text>                                       