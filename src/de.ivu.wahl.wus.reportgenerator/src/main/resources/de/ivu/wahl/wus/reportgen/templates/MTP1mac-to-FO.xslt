:if>                                   <fo:block>                                      <fo:leader leader-pattern="space"/>                                 </fo:block>                                 <fo:inline-container>                                       <fo:block>                                          <xsl:text>&#x200B;</xsl:text>                                       </fo:block>                                 </fo:inline-container>                                  <fo:block font-family="Arial" font-size="larger" font-weight="bold" margin-right="100% - 100%" margin="0pt">                                        <fo:inline font-family="Arial">                                         <xsl:text>7.    Vergoedingen</xsl:text>                                     </fo:inline>                                    </fo:block>                                 <fo:block>                                      <fo:leader leader-pattern="space"/>                                 </fo:block>                                 <fo:inline-container>                                       <fo:block>                                          <xsl:text>&#x200B;</xsl:text>                                       </fo:block>                                 </fo:inline-container>                                  <fo:block font-family="Arial" margin-right="100% - 100%" margin="0pt">                                      <fo:inline-container>                                           <fo:block>                                              <xsl:text>&#x200B;</xsl:text>                                           </fo:block>                                     </fo:inline-container>                                      <fo:table font-family="Arial" table-layout="fixed" width="100%" border-spacing="2pt">                                           <fo:table-column column-width="30pt"/>                                          <fo:table-column column-width="proportional-column-width(1)"/>                                          <xsl:variable name="altova:CurrContextGrid_23" select="."/>                                         <fo:table-body font-family="Arial" start-indent="0pt">                                              <xsl:variable name="altova:tablerows">                                                  <fo:table-row font-family="Arial">                                                      <fo:table-cell font-family="Arial" padding="2pt" display-align="before">                                                            <fo:block text-align="left">                                                                <fo:inline font-family="Arial">                                                                 <xsl:text>7.1</xsl:text>                                                                </fo:inline>                                                            </fo:block>                                                     </fo:table-cell>                                                        <fo:table-cell font-family="Arial" padding="2pt" display-align="before">                                                            <fo:block text-align="left">                                                                <fo:inline font-family="Arial">                                                                 <xsl:text>  Opdrachtnemer en Opdrachtgever komen, met betrekking tot vergoeding voor de in de krachtens deze Helpdesk overeenkomst te leveren diensten, het volgende overeen:</xsl:text>                                                                </fo:inline>                                                                <fo:block/>                                                             <fo:inline-container>                                                                   <fo:block>                                                                      <xsl:text>&#x200B;</xsl:text>                 